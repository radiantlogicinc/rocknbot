        {
            "name": "LilLisa_Server - Main",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/LilLisa_Server/src/main.py",
            "cwd": "${workspaceFolder}/LilLisa_Server",
            "python": "${workspaceFolder}/LilLisa_Server/.venv/bin/python",
